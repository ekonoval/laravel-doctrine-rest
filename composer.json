{
    "name": "pavelz/laravel-doctrine-rest",
    "description": "Doctrine Rest - Laravel implementation",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "type": "library",
    "license": "MIT",
    "scripts": {
        "post-install-cmd": "./vendor/phpunit/phpunit/phpunit --config phpunit.xml"
    },
    "authors": [
        {
            "name": "PavelZ",
            "email": "r3volut1oner@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Pz\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pz\\LaravelDoctrine\\Rest\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "laravel-doctrine/orm": "^2.0",
        "laravel-doctrine/migrations": "^3.1",
        "laravel/laravel": "^11.0",
        "phpunit/phpunit": "^10.5"
    },
    "require": {
        "php": "^8.2",
        "pz/doctrine-rest": "^0.5.0"
    }
}
